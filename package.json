{
  "name": "dreeam-format",
  "version": "0.4.14",
  "description": "",
  "main": "dist/npm/index.js",
  "scripts": {
    "start": "budo src/index.js:js/bundle.js --dir public/ --open --port 4021 -- -t babelify -t [cssify --global=true ]",
    "rename-bundle": "mv public/js/bundle.js public/js/bundle.$npm_package_version.js",
    "cache-bust": "replace-in-file bundle.js bundle.$npm_package_version.js public/index.html",
    "uglify": "uglifyjs browserified.js --compress drop_console=true --mangle --output public/js/bundle.js",
    "browserify": "browserify src/index.js --outfile browserified.js -t [ babelify ] -t [cssify --global=true ] -g [envify --NODE_ENV production]",
    "prebuild": "rimraf public/js/*.js browserified.js",
    "build": "npm run browserify && npm run uglify && npm run rename-bundle && npm run cache-bust && npm run build-npm",
    "build-npm": "babel src/npm -d dist/npm && babel ./src/languages -d ./dist/languages",
    "serve-prod": "budo --dir public/ --open",
    "test": "jest --coverage",
    "check": "depcheck --ignores=\"depcheck,babelify,cssify,envify,babel-cli\"",
    "release": "npm run test && standard-version"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "ajv": "^6.5.5",
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.2.3",
    "cssify": "^1.0.3",
    "cuid": "^2.1.4",
    "envify": "^4.1.0",
    "fakerator": "^0.3.0",
    "geojson-random": "^0.4.0",
    "i18next": "^10.0.7",
    "json-schema-defaults": "^0.4.0",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.4",
    "nomnoml": "^0.2.0",
    "promise-json-file-reader": "^1.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-i18next": "^6.1.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "uel": "0.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "budo": "^11.2.0",
    "depcheck": "^0.6.9",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "fetch-mock": "^5.13.1",
    "jest": "^22.4.3",
    "standard-version": "^4.3.0"
  }
}
