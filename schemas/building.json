              "description": "Summarizing VPElVent, VPElRoom, VPElTVV"
            },
            "electricitySupplyIF": {
              "$id": "electricitySupplyIF",
              "title": "Electricity Supply Intake Fan?",
              "type": "number",
              "description": "Not sure"
            },
            "electricitySupplySCP": {
              "$id": "electricitySupplySCP",
              "title": "Electricity Supply Solar Circulation Pump",
              "type": "number",
              "description": "Electricity from solar circulation pump"
            },
            "heatPumpHeatExchangeRoom": {
              "$id": "heatPumpHeatExchangeRoom",
              "title": "Heat Pump Heat Exchange Room",
              "type": "number",
              "description": ""
            },
            "heatPumpHeatExchangeVent": {
              "$id": "heatPumpHeatExchangeVent",
              "title": "Heat Pump Heat Exchange Ventilation",
              "type": "number",
              "description": ""
            },
            "heatPumpHeatRecovery": {
              "$id": "heatPumpHeatRecovery",
              "title": "Heat Pump Heat Recovery",
              "type": "number",
              "description": "This is used as main post in gained energy"
            },
            "heatStorage": {
              "$id": "heatStorage",
              "title": "Heat Storage",
              "type": "number",
              "description": "Storage of heat in the building body"
            },
            "heating": {
              "$id": "heating",
              "title": "Heating",
              "type": "number",
              "description": "Heating summarized by heatingRoom, heatingVent, heatingHotw. Main property in gained balance"
            },
            "heatingHotw": {
              "$id": "heatingHotw",
              "title": "Heating Hot Water",
              "type": "number",
              "description": "Heating demand for hot water"
            },
            "heatingRoom": {
              "$id": "heatingRoom",
              "title": "Heating Room",
              "type": "number",
              "description": "Heating demand for room air"
            },
            "heatingVent": {
              "$id": "heatingVent",
              "title": "Heating Ventilation",
